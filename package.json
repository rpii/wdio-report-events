    "rimraf": "^3.0.2",
    "typescript": "^3.8.3",
    "tslint": "6.1.0",
    "prettier": "1.19.1",
    "tslint-config-prettier": "1.18.0",
    "chai": "^4.2.0",
    "mocha": "^7.0.0",
    "ts-node": "^8.8.1",
    "ts-mocha": "6.0.0",
    "tsconfig-paths": "3.9.0",
    "@types/node": "^13.1.7",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^5.2.7"
