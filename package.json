{
  "name": "@rpii/wdio-report-events",
  "version": "0.1.0",
  "description": "Abstracts the event driven messaging to pass screenshots and log data to webdriverio reports",
  "main": "lib/wdio-report-events",
  "types": "lib/wwdio-report-events.d.ts",
  "scripts": {
    "clean": "rimraf ./lib",
    "build": "tsc",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register tests/**/*.test.ts",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json --project",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rpii/wdio-report-events.git"
  },
  "keywords": [
    "webdriverio",
    "data",
    "driven",
    "test"
  ],
  "author": "rpii",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rpii/wdio-report-events/issues"
  },
  "homepage": "https://github.com/rpii/wdio-report-events#readme",
  "devDependencies": {
    "rimraf": "^3.0.0",
    "typescript": "^3.7.4",
    "tslint": "5.20.1",
    "prettier": "1.19.1",
    "tslint-config-prettier": "1.18.0",
    "chai": "^4.2.0",
    "mocha": "^7.0.0",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "3.9.0",
    "@types/node": "^13.1.7",
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7"
  },
  "dependencies": {
    "xlsx": "^0.15.3"
  },
  "mocha": {
    "ui": "tdd"
  }
}
